{
  "__comment__": "An example configuration for align_csv.py based on the isa:biosample table in FaceBase",
  "schema": "isa",
  "table": "biosample",
  "null": "NA",
  "mappings": {
    "dataset": "{Dataset}",
    "local_identifier": "{Biosample}",
    "species": "NCBITAXON:10090",
    "specimen": "FACEBASE:1-4GNW",
    "gene": {
        "skip": ["NA"],
        "separator": ";",
        "query": "/attribute/gene:=vocab:gene/gene_alternate_id:=isa:gene_alternate_id/gene_alternate_id:Alternate_ID={Gene_MGI_ID}/gene:id",
        "value": "{id}"
    },
    "genotype": {
        "skip": ["NA"],
        "query": "/attribute/vocab:genotype/name={Genotype}/id",
        "value": "{id}"
    },
    "strain": {
        "skip": ["NA"],
        "query": "/attribute/vocab:strain/id={Strain_MGI_ID}/id",
        "value": "{id}"
    },
    "stage": {
        "skip": ["NA"],
        "query": "/attribute/vocab:stage/name={Stage}/id",
        "value": "{id}"
    },
    "anatomy": "UBERON:0000033",
    "sex": {
        "skip": ["NA"],
        "value": "{Sex}",
        "replace": {
            "F": "UBERON:0003100",
            "M": "UBERON:0003101"
        }
    }
  }
}
